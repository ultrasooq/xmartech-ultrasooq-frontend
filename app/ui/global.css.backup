}

.existing-product-add-wrapper {
  display: flex;
  column-gap: 24px;
  width: calc(100% + calc(282px + 24px));
  & .left-content {
    width: calc(100% - calc(282px + 24px));
  }

  /* start: responsive */

  /* end: responsive */
}

.left-content {
  .existing-product-add-wrapper & {
    width: calc(100% - calc(282px + 24px));
  }

  /* start: responsive */

  /* end: responsive */
}

.m-auto {
  /* start: responsive */
  @media (max-width: 1620px) {
    .existing-product-add-page &.container.px-3 {
      max-width: unset !important;
      padding-left: 32px !important;
      padding-right: 32px !important;
    }
  }

  /* end: responsive */
}

.px-3 {
  /* start: responsive */
  @media (max-width: 1620px) {
    .existing-product-add-page &.container.m-auto {
      max-width: unset !important;
      padding-left: 32px !important;
      padding-right: 32px !important;
    }
  }

  /* end: responsive */
}

.show {
  /* start: responsive */

  @media (max-width: 1024.98px) {
    .trending-search-sec &.left-filter {
      transform: translate(0);
    }

    .rfq_section .rfq_main_box &.rfq_left {
      transform: translate(0);
    }

    .trending-search-sec &.left-filter ~ .left-filter-overlay {
      @apply fixed left-0 top-0 z-40 block h-full w-full;
    }

    .rfq_section .rfq_main_box &.rfq_left ~ .left-filter-overlay {
      @apply fixed left-0 top-0 z-40 block h-full w-full;
    }
  }

  /* end: responsive */
}

.border {
  /* start: responsive */

  @media (max-width: 991.98px) {
    .body-content-s1 &.main-content-card .table-responsive {
      @apply p-2.5 !pb-0;
    }
  }

  /* end: responsive */
}

.auth-page-box {
  /* start: responsive */

  @media (max-width: 767.98px) {
    .auth-page-main & {
      @apply px-4 py-6;
    }

    .auth-page-main & h2 {
      @apply mb-2 text-2xl leading-[30px];
    }
