.theme-primary-btn {
  /* start: theme-primary-btn */
  @apply !bg-primary text-[16px] text-white;
  border-radius: 2px;
  @apply !relative !z-10 !flex !cursor-pointer !items-center !justify-center !overflow-hidden !text-center !font-medium !capitalize;
  transition: all 0.5s !important;

  &::before {
    transition: all 0.5s !important;
    @apply z-[-1];
  }

  &::after {
    transition: all 0.5s !important;
    @apply z-[-1];
  }

  &:hover {
    @apply text-white shadow-[1px_3px_12px_#afafaf];
  }

  &:before {
    @apply absolute top-2/4 h-5 w-5 rounded-[50%] bg-secondary content-[''];
  }

  &:after {
    @apply absolute top-2/4 h-5 w-5 rounded-[50%] bg-secondary content-[''];
  }

  &:before {
    @apply -left-5 -translate-x-2/4 -translate-y-2/4;
    -webkit-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
  }

  &:after {
    @apply -right-5 -translate-y-2/4 translate-x-2/4;
    -webkit-transform: translate(50%, -50%);
    -ms-transform: translate(50%, -50%);
  }

  &:hover:before {
    -webkit-animation: criss-cross-left 0.8s both;
    animation: criss-cross-left 0.8s both;
  }

  &:hover:after {
    -webkit-animation: criss-cross-right 0.8s both;
    animation: criss-cross-right 0.8s both;
  }

  .theme-pagination-s1 &button {
    @apply !h-[30px] !w-[50px] !gap-x-1.5 !rounded-[1.96px] !p-0 !text-xs;
  }

  .manage_product_action &.custom-btn {
