  /* end: responsive */
}

.text-base {
  /* start: responsive */

  @media (max-width: 575px) {
    @apply text-sm leading-[22px];
  }

  /* end: responsive */
}

@layer base {
  :root {
    --background: 0 0% 100%;
    --foreground: 222.2 84% 4.9%;

    --card: 0 0% 100%;
    --card-foreground: 222.2 84% 4.9%;

    --popover: 0 0% 100%;
    --popover-foreground: 222.2 84% 4.9%;

    --primary: 222.2 47.4% 11.2%;
    --primary-foreground: 210 40% 98%;

    --secondary: 210 40% 96.1%;
    --secondary-foreground: 222.2 47.4% 11.2%;

    --muted: 210 40% 96.1%;
    --muted-foreground: 215.4 16.3% 46.9%;

    --accent: 210 40% 96.1%;
    --accent-foreground: 222.2 47.4% 11.2%;

    --destructive: 0 84.2% 60.2%;
    --destructive-foreground: 210 40% 98%;

    --border: 214.3 31.8% 91.4%;
    --input: 214.3 31.8% 91.4%;
    --ring: 222.2 84% 4.9%;

    --radius: 0.5rem;
    /* theme colors */
    --theme-primary-color: #db2302;
    --theme-secondary-color: #022335;
    --theme-gray-light: #f3f3f3;
    --theme-gray-dark: #e8e8e8;
    --white-color: #fff;
  }

  .dark {
    --background: 222.2 84% 4.9%;
    --foreground: 210 40% 98%;

    --card: 222.2 84% 4.9%;
    --card-foreground: 210 40% 98%;

    --popover: 222.2 84% 4.9%;
    --popover-foreground: 210 40% 98%;

    --primary: 210 40% 98%;
    --primary-foreground: 222.2 47.4% 11.2%;

    --secondary: 217.2 32.6% 17.5%;
    --secondary-foreground: 210 40% 98%;

    --muted: 217.2 32.6% 17.5%;
    --muted-foreground: 215 20.2% 65.1%;

    --accent: 217.2 32.6% 17.5%;
    --accent-foreground: 210 40% 98%;

    --destructive: 0 62.8% 30.6%;
    --destructive-foreground: 210 40% 98%;

    --border: 217.2 32.6% 17.5%;
    --input: 217.2 32.6% 17.5%;
    --ring: 212.7 26.8% 83.9%;
  }
}

@layer base {
  * {
    @apply border-border;
  }

  body {
    @apply bg-background text-foreground;
  }

  input[type="number"]::-webkit-inner-spin-button,
  input[type="number"]::-webkit-outer-spin-button {
    -webkit-appearance: none;
    margin: 0;
  }
}

@layer components {
  @-webkit-keyframes criss-cross-right {
    0% {
      right: -20px;
    }

    50% {
      right: 50%;
      width: 20px;
      height: 20px;
    }

    100% {
      right: 50%;
      width: 875px;
      height: 875px;
    }
  }

  @keyframes criss-cross-right {
    0% {
      right: -20px;
    }

    50% {
      right: 50%;
      width: 20px;
      height: 20px;
    }

    100% {
      right: 50%;
      width: 875px;
      height: 875px;
    }
  }

  @-webkit-keyframes criss-cross-left {
    0% {
      left: -20px;
    }

    50% {
      left: 50%;
      width: 20px;
      height: 20px;
    }

    100% {
      left: 50%;
      width: 875px;
      height: 875px;
    }
  }

  @keyframes criss-cross-left {
    0% {
      left: -20px;
    }

    50% {
      left: 50%;
      width: 20px;
      height: 20px;
    }

    100% {
      left: 50%;
      width: 375px;
      height: 375px;
    }
  }

  input:focus {
    box-shadow: none !important;
    @apply !border-primary;
  }
}

@layer utilities {
  .react-time-picker__wrapper {
    display: flex;
    height: 100%;
    align-items: center;
    padding: 0 12px;
    justify-content: space-between;
  }

  /* width */
  ::-webkit-scrollbar {
    width: 6px;
  }

  /* Track */
  ::-webkit-scrollbar-track {
    box-shadow: inset 0 0 3px grey;
    border-radius: 10px;
  }

  /* Handle */
  ::-webkit-scrollbar-thumb {
    background: var(--theme-primary-color);
    border-radius: 10px;
  }

  /* Handle on hover */
  ::-webkit-scrollbar-thumb:hover {
    background: var(--theme-primary-color);
  }

  /* browser password toggle visibility */
  input::-ms-reveal,
  input::-ms-clear {
    display: none;
  }

  .horizontal-slider {
    width: 100%;
    max-width: 500px;
    height: 60px;
    margin: auto;
  }

  .example-thumb {
    cursor: pointer;
    position: absolute;
    z-index: 100;
    background: #ffffff;
    border: 10px solid #3774ff;
    border-radius: 100%;
    display: block;
    box-shadow: 0 0 2px 0 rgb(0 0 0 / 44%);
  }

  .example-thumb.active {
    background-color: grey;
  }

  .example-track {
    position: relative;
    background: #ddd;
  }

  .example-track.example-track-1 {
    background: #83a9ff;
  }

  .horizontal-slider .example-track {
    top: 20px;
    height: 4px;
  }

  .horizontal-slider .example-thumb {
    top: 12px;
    width: 10px;
    outline: none;
    height: 10px;
    line-height: 38px;
  }

  .player-wrapper {
    position: relative;
    /* height: 50%; */
    padding-top: 5%;
  }

  .react-player {
    position: absolute;
    top: 0;
    left: 0;
  }

  .paginate-button a {
    @apply flex h-[30px] w-[30px] items-center justify-center rounded-[4px] border-solid border-[#F8F5F5] bg-[#F8F5F5] text-[13.261px] font-normal leading-[normal] tracking-[-0.029px] text-[#A8A8A8];
  }

  .active-paginate-button a {
    @apply h-[30px] w-[30px] border-primary bg-primary text-white;
  }

  .dropdown {
    position: relative;
    display: inline-block;
  }

  .dropbtn {
    color: white;
    padding: 16px;
    font-size: 16px;
    border: none;
  }

  .dropdown-content {
    display: none;
    position: absolute;
    background-color: #fff;
    min-width: 280px;
    box-shadow: 0px 8px 8px 0px rgba(0, 0, 0, 0.2);
    z-index: 100;
    height: 432px;
    overflow-y: auto;
  }

  .dropdown:hover .dropdown-content {
    display: block;
  }

  .dropdown-content-child:hover {
    background-color: #f1f1f1;
  }

  .dropdown-active-child {
    background-color: #f1f1f1;
  }

  .dropdown-content-second {
    display: none;
    position: absolute;
    background-color: #fff;
    min-width: 280px;
    box-shadow: 0px 8px 8px 0px rgba(0, 0, 0, 0.2);
    z-index: 100;
    left: 280px;
    height: 432px;
    overflow-y: auto;
  }

  .dropdown:hover .dropdown-content-second {
    display: block;
  }

  .dropdown-content-third {
    display: none;
    position: absolute;
    background-color: #fff;
    min-width: 560px;
    box-shadow: 0px 8px 8px 0px rgba(0, 0, 0, 0.2);
    z-index: 100;
    left: 560px;
    height: 380px;
    overflow-y: auto;
  }

  .dropdown:hover .dropdown-content-third {
    display: block;
  }


}

/* Team Members Rules - Outside all @layer blocks */
.team_members_section {
  @apply w-full py-7;
}

.team_members_heading {
  @apply w-full flex items-center justify-between mb-4;
}

.team_members_heading h1 {
  @apply text-4xl font-semibold leading-10 text-gray-900;
}

.team_members_heading button {
  @apply w-auto bg-[#db2302] rounded-sm p-3 text-center text-white capitalize font-semibold flex items-center justify-center;
}

.team_members_table {
  @apply w-full rounded-lg border border-solid border-[#E4E3E3] bg-white py-5 px-4;
}

.team_members_table table {
  @apply w-full border-none;
  box-shadow: 0px 12px 40px 0px rgba(255, 69, 35, 0.07);
}

.team_members_table table thead {
  @apply w-full bg-[#F3F3F3] border border-solid border-[#E8E8E8] rounded-sm;
}

.team_members_table table thead tr th {
  @apply p-[20px] text-sm font-medium text-[#333] text-left;
}

.team_members_table table tbody tr td {
  @apply p-[20px] border-b border-solid border-[#E8E8E8] text-sm font-normal text-[#7F818D];
}

.team_members_table table thead tr th:nth-child(4),
.team_members_table table tbody tr td:nth-child(4) {
  @apply w-[20%] whitespace-normal break-words break-all;
}

.team_members_table table thead tr th:nth-child(5),
.team_members_table table tbody tr td:nth-child(5) {
  @apply text-center;
}
